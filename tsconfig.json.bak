{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
  "module": "es2015",
    "lib": [
    "ES2022",
    "DOM",
    "DOM.Iterable"
    ],
    "skipLibCheck": true,
  // "types": ["node"],
  "typeRoots": ["./node_modules/@types"],
  "moduleResolution": "node",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}